# Database Configuration
# Name of the SQLite database file
DATABASE_FILE_NAME=database.db

# Local LLM Configuration
# Base URL for your local LLM API (e.g., llama.cpp, Ollama, vLLM)
LOCAL_LLM_URL=http://localhost:8080/v1
# API key for the local LLM (set to "not-needed" if not required)
LOCAL_LLM_API_KEY=not-needed
# Model name to use for inference
LOCAL_LLM_MODEL=Qwen3-Vl-4B-Instruct

# Application Settings
# Default number of items to return in search results
DEFAULT_LIMIT_COUNT=20